{
  "$schema": "http://json.schemastore.org/proxies",
  "proxies": {
    "appResources": {
      "matchCondition": {
        "route": "/static/{*rest}",
        "methods": [
          "GET",
          "HEAD",
          "OPTIONS"
        ]
      },
      "backendUri": "https://stephenclearyapps.github.io/DotNetApis/static/{rest}"
    },
    "app": {
      "matchCondition": {
        "route": "/{*rest}",
        "methods": [
          "GET",
          "HEAD",
          "OPTIONS"
        ]
      },
      "backendUri": "https://stephenclearyapps.github.io/DotNetApis/"
    },
    "api": {
      "matchCondition": {
        "route": "/api/{*rest}",
        "methods": [
          "GET",
          "HEAD",
          "OPTIONS"
        ]
      },
      "backendUri": "https://%WEBSITE_HOSTNAME%/api/{rest}"
    }
  }
}