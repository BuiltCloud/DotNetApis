{
  "$schema": "http://json.schemastore.org/proxies",
  "proxies": {
    "app": {
      "matchCondition": {
        "route": "{*url}",
        "methods": [
          "GET",
          "HEAD",
          "OPTIONS"
        ]
      },
      "backendUri": "https://dotnetapisapp.stephencleary.com/"
    },
    "api": {
      "matchCondition": {
        "route": "/api/{*url}",
        "methods": [
          "GET",
          "HEAD",
          "OPTIONS"
        ]
      },
      "backendUri": "https://%WEBSITE_HOSTNAME%/api/{url}"
    }
  }
}